<mvc:View controllerName="com.sap.ui5tutorial.controller.Main"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:core="sap.ui.core">
    <f:DynamicPage id="dynamicMasterPageId" headerExpanded="true" toggleHeaderOnTitleClick="true" busy="{viewModel>/busy}" class="sapUiSizeCompact">
        <!-- DynamicPage Title - Always visible -->
        <f:title>
            <f:DynamicPageTitle>
                <f:heading>
                    <HBox alignItems="Center">
                        <Title text="Northwind Products" />
                    </HBox>
                </f:heading>
            </f:DynamicPageTitle>
        </f:title>
        <!-- DynamicPage Header - Collapsible and Expandable -->
        <f:header>
            <f:DynamicPageHeader pinnable="true">
                <core:Fragment fragmentName="com.sap.ui5tutorial/fragments/Master/Filter" type="XML"/>
            </f:DynamicPageHeader>
        </f:header>
        <!-- DynamicPage Content -->
        <f:content>
            <core:Fragment fragmentName="com.sap.ui5tutorial/fragments/Master/Table" type="XML"/>
        </f:content>
        <!-- DynamicPage Footer - Optional -->
        
    </f:DynamicPage>
</mvc:View>
